{
    "workbench.colorCustomizations": {
        "editorWhitespace.foreground": "#707070",
        "editorLineNumber.foreground": "#707070",
        "terminal.foreground": "#abb2bf",
        "terminal.background": "#282c34",
        "terminal.ansiBlack": "#1e2127",
        "terminal.ansiBlue": "#61afef",
        "terminal.ansiBrightBlack": "#5c6370",
        "terminal.ansiBrightBlue": "#61afef",
        "terminal.ansiBrightCyan": "#56b6c2",
        "terminal.ansiBrightGreen": "#98c379",
        "terminal.ansiBrightMagenta": "#c678dd",
        "terminal.ansiBrightRed": "#e06c75",
        "terminal.ansiBrightWhite": "#e6efff",
        "terminal.ansiBrightYellow": "#d19a66",
        "terminal.ansiCyan": "#56b6c2",
        "terminal.ansiGreen": "#98c379",
        "terminal.ansiMagenta": "#c678dd",
        "terminal.ansiRed": "#e06c75",
        "terminal.ansiWhite": "#828791",
        "terminal.ansiYellow": "#d19a66",
    },
    "editor.renderWhitespace": "all",
    "editor.tokenColorCustomizations": {
        "[Atom One Dark]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "comment.line"
                    ],
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#98C379"
                    }
                },
                {
                    "name": "comment_punc",
                    "scope": "punctuation.definition.comment",
                    "settings": {
                        "foreground": "#98C379",
                        "fontStyle": "bold"
                    }
                },
                {
                    "name": "String",
                    "scope": [
                        "string"
                    ],
                    "settings": {
                        "foreground": "#00e500"
                    }
                },
            ]
        }
    },
    "window.zoomLevel": 0,
    "window.titleBarStyle": "custom",
    "editor.fontFamily": "Fira Code, Hack, Source Code Pro, Courier New",
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "rust-analyzer.updates.askBeforeDownload": false,
    "rust-analyzer.checkOnSave.extraArgs": [
        "--target-dir",
        "target/ra-check"
    ],
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.selectionClipboard": false,
    "editor.formatOnSave": true,
    "editor.rulers": [
        80,
        120
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "extensions.ignoreRecommendations": true,
    "workbench.colorTheme": "Atom One Dark",
    "breadcrumbs.enabled": true,
    "shellcheck.executablePath": "shellcheck",
    "shellcheck.enable": true,
    "todohighlight.include": [
        "**/*.rs"
    ],
    "todohighlight.keywords": [
        "panic!(",
        {
            "text": "panic!(",
            "color": "#ff0000",
            "backgroundColor": "black",
            "overviewRulerColor": "grey"
        },
        "expect(",
        {
            "text": "expect(",
            "color": "#ff0000",
            "backgroundColor": "black",
            "overviewRulerColor": "grey"
        },
        "unwrap()",
        {
            "text": "unwrap()",
            "color": "#ff0000",
            "backgroundColor": "black",
            "overviewRulerColor": "grey"
        },
    ],
    "peacock.affectActivityBar": true,
    "peacock.affectStatusBar": true,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        },
        {
            "name": "greenish",
            "value": "#667e44"
        },
        {
            "name": "purple",
            "value": "#5c5d85"
        }
    ],
    "git.suggestSmartCommit": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/target/**": true
    },
    "terminal.integrated.fontFamily": "Fira Code",
    "workbench.startupEditor": "newUntitledFile",
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)"
}